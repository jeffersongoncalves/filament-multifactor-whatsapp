{
  "name": "jeffersongoncalves/filament-multifactor-whatsapp",
  "description": "Filament 4 multi‑factor authentication via WhatsApp one‑time codes, with optional Evolution API connector integration.",
  "keywords": [
    "jeffersongoncalves",
    "laravel",
    "filament",
    "filament-plugin",
    "mfa",
    "2fa",
    "two-factor",
    "multi-factor",
    "whatsapp",
    "otp",
    "one-time-code",
    "authentication",
    "filament-multifactor-whatsapp",
    "evolution-api",
    "whatsapp-connector"
  ],
  "homepage": "https://github.com/jeffersongoncalves/filament-multifactor-whatsapp",
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson Gonçalves",
      "email": "gerson.simao.92@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0",
    "spatie/laravel-package-tools": "^1.14.0",
    "wallacemartinss/filament-whatsapp-conector": "^1.0"
  },
  "require-dev": {
    "larastan/larastan": "^3.0",
    "laravel/pint": "^1.21",
    "orchestra/testbench": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "JeffersonGoncalves\\Filament\\MultiFactorWhatsApp\\": "src"
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JeffersonGoncalves\\Filament\\MultiFactorWhatsApp\\MultiFactorWhatsappServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
